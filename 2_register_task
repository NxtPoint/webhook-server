}
headers = {
    "Authorization": "Bearer qA3X6Tg6Ac8Gixyqv7eQTz999zoXvgRDlFTryanrST",
    "Content-Type": "application/json"
}

response = requests.request("POST", url, json=payload, headers=headers, params=querystring)

print(response.text)